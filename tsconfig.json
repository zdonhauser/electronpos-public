{
    "compilerOptions": {
      "target": "ES6",
      "module": "CommonJS",
      "jsx": "react",
      "outDir": "./dist",
      "strict": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["./"],
    "exclude": ["node_modules", "dist", "posdashboard","out"]
  }
  