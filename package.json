{
  "name": "zdt_pos",
  "version": "2025.1.31",
  "description": "pos",
  "main": "./dist/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "start": "cd posdashboard && yarn build && cd .. && yarn build && electron-forge start",
    "quickstart": "yarn build && electron-forge start",
    "startall": "concurrently \"cd posdashboard && npx tsc && cd client && yarn start-local\" \"wait-on http://localhost:3001 && yarn build && electron-forge start\"",
    "package": "electron-forge package",
    "makeall": "yarn build && cd posdashboard && npx tsc && cd client && yarn build && electron-forge make",
